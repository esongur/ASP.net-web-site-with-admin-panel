@model IEnumerable<Ertunc_Tibbi_Cihaz_Web_Site.Models.Model.Sertifikalar>
@{
    ViewBag.Title = "Sertifikalar";
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}

<section id="inner-headline">
    <div class="container">
        <div class="row">
            <div class="span4">
                <div class="inner-heading">
                    <h2>Sertifikalar</h2>
                </div>
            </div>
            <div class="span8">
                <ul class="breadcrumb">
                    <li><a href="/Anasayfa"><i class="icon-home"></i></a><i class="icon-angle-right"></i></li>
                    <li class="active">Sertifikalar</li>
                </ul>
            </div>
        </div>
    </div>
</section>

<section id="content">
    <div class="container">
        <div class="row">
            <div class="span3">
                <aside class="left-sidebar">
                    <div class="widget">
                        @{
                            Html.RenderPartial("KurumsalMenuPartial", "Home");
                        }
                    </div>

                </aside>
            </div>
            <section id="content">
                <div class="container">
                    <div class="row">
                        <div class="span8">
                            <h4><strong>Sertifikalar</strong></h4>
                            <table class="border">
                                <tr>
                                    
                                
                                <td>
                                    @foreach (var item in Model)
                                    {

                                        <img src="@item.ResimURL" height="150" width="150" class="bordered">

                                    }

                                </td>
                                </tr>
                            </table>


                        </div>
                    </div>

                </div>
            </section>
        </div>
    </div>
    </div>
</section>


@{
    Html.RenderAction("FooterPartial", "Home");}
