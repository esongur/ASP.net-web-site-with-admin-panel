@using PagedList;
@using PagedList.Mvc;
@using Ertunc_Tibbi_Cihaz_Web_Site.Helper;
@model IEnumerable<Ertunc_Tibbi_Cihaz_Web_Site.Models.Model.Urunler>
@foreach (var item in Model)
{
    <ul class="recent">
        <li>
            <img src="@item.UrunlerResimURL1" class="pull-left" alt="@item.UrunlerBaslik" />
            <h6><a href="/Ürünler/@SeoHelper.ToSeoUrl(item.UrunlerBaslik).ToLower()-@item.UrunlerId/">@item.UrunlerBaslik</a></h6>
            <p>
                @Html.Raw(item.UrünlerAciklama)
            </p>
        </li>

    </ul>

}
