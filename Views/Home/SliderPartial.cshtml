@model IEnumerable<Ertunc_Tibbi_Cihaz_Web_Site.Models.Model.Slider>
@{
    Layout = null;

}


<div class="container">
    <div class="slider-wrapper theme-default">
        <div id="slider" class="nivoSlider">
            @foreach (var item in Model)
            {
                <img src="@item.ResimURL" data-thumb="@item.ResimURL" alt="" class="img-fluid" />
            }

        </div>
    </div>
</div>


